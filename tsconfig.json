{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "target": "es2017",
    "baseUrl": "./",
    "strict": true
  },
  "include": [
    "./**/*"
  ]
}
